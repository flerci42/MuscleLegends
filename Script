                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4100.19971, 1020.82947, -3801.42603)
                local selectedMachine = "Bench Press" == "Legends Press"
            end
            if selectedMachine == "Pullups" and selectedMachineLocation == "Spawn" then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-186.290375, 3.6768918, 120.979874)
            end
            if selectedMachine == "Pullups" and selectedMachineLocation == "Beach" then
                OrionLib:MakeNotification({
                    Name = "ALERT",
