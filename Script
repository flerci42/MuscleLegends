    Name = "Strength",
    Default = "Numbers Only",
    TextDisappear = false,
    Callback = function(v)
        selectedStrength = v
    end
})

local selectedRebirths
Tab:AddTextbox({
    Name = "Rebirths",
    Default = "Numbers Only",
    TextDisappear = false,
    Callback = function(v)
        selectedRebirths = v
    end
})

local selectedKills
Tab:AddTextbox({
    Name = "Kills",
    Default = "Numbers Only",
    TextDisappear = false,
    Callback = function(v)
        selectedKills = v
    end
})

local selectedBrawls
Tab:AddTextbox({
    Name = "Brawls",
    Default = "Numbers Only",
    TextDisappear = false,
    Callback = function(v)
        selectedBrawls = v
    end
})

Tab:AddButton({
    Name = "Confirm",
    Default = false,
    Callback = function(v)
        game:GetService("Players").flerci42.leaderstats.Strength.Value = selectedStrength
        game:GetService("Players").flerci42.leaderstats.Rebirths.Value = selectedRebirths
        game:GetService("Players").flerci42.leaderstats.Kills.Value = selectedKills
        game:GetService("Players").flerci42.leaderstats.Brawls.Value = selectedBrawls
    end
})

local Tab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://7549514426",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Gyms"
})

local selectedGym
Tab:AddDropdown({
    Name = "Choose Gym",
    Default = "1",
    Options = {"Beach", "Frost Gym", "Mythical Gym", "Eternal Gym", "Legends Gym", "Muscle King"},
    Callback = function(v)
        selectedGym = v
    end
})

Tab:AddButton({
    Name = "Teleport",
    Default = false,
    Callback = function(v)
        if selectedGym == "Beach" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3.90393853, 3.67689133, -368.555298)
        end
        if selectedGym == "Frost Gym" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2588.98706, 3.62689185, -407.025208)
        end
        if selectedGym == "Mythical Gym" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2226.30859, 3.62689161, 1073.52307)
        end
        if selectedGym == "Eternal Gym" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-6735.47119, 3.62689114, -1285.62024)
        end
        if selectedGym == "Legends Gym" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4630.44434, 987.829895, -3896.14648)
        end
        if selectedGym == "Muscle King" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-8584.51367, 13.5269194, -5679.39795)
        end
    end
})

