    Name = "Teleport",
    Default = false,
    Callback = function(v)
        if selectedGym == "Tiny Island" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-37.2061043, 3.83023095, 1879.30176)
        end
        if selectedGym == "Spin Wheel" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2604.21094, -2.31000328, 5740.63574)
        end
    end
})

