local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "ABLEYM HUB", HidePremium = false, SaveConfig = true, IntroText = "ABLEYM HUB"})

local Tab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://7495464623",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Auto Farm [TOOLS]"
})

Tab:AddDropdown({
    Name = "Choose Item",
    Default = "1",
    Options = {"Weight", "Pushups", "Situps", "Handstands"},
    Callback = function(v)
        selectedTool = v
    end
})

Tab:AddToggle({
    Name = "Auto Farm",
    Default = false,
    Callback = function(v)
        getgenv().autoFarm = v
        while true do
            if not  getgenv().autoFarm then return end
        if selectedTool == "Weight" or
        selectedTool == "Pushups" or
        selectedTool == "Situps" or
        selectedTool == "Handstands" then
            local args = {
                [1] = "rep"
            }
            game:GetService("Players").LocalPlayer.muscleEvent:FireServer(unpack(args))
        wait(0.3)
    end
    end
end
})

local Section = Tab:AddSection({
	Name = "Auto Farm [MACHINES]"
})

local selectedMachine
Tab:AddDropdown({
    Name = "Choose Machine",
    Default = "1",
    Options = {"Bench Press", "Pullups", "Squat Rack", "Deadlift", "Boulder Throw"},
    Callback = function(v)
        selectedMachine = v
    end
})

local selectedMachineLocation
Tab:AddDropdown({
    Name = "Choose Machine Location",
    Default = "1",
    Options = {"Spawn", "Beach", "Frost Gym", "Mythical Gym", "Eternal Gym", "Legends Gym"},
    Callback = function(v)
        selectedMachineLocation = v
    end
})

Tab:AddToggle({
    Name = "Auto Farm",
    Default = false,
    Callback = function(v)
        getgenv().autoFarm = v
