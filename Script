            if not  getgenv().autoFarm then return end
            if selectedMachine == "Bench Press" and selectedMachineLocation == "Spawn" then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-193.448639, 3.67689228, 286.716614)
            end
            if selectedMachine == "Bench Press" and selectedMachineLocation == "Beach" then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(192.52243, 3.67689133, -330.209106)
            end
            if selectedMachine == "Bench Press" and selectedMachineLocation == "Frost Gym" then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2623.21021, 3.67689157, -174.649857)
                local selectedMachine = "Bench Press" == "Frost Press"
            end
            if selectedMachine == "Bench Press" and selectedMachineLocation == "Mythical Gym" then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2369.99243, 39.5785675, 1228.28259)
